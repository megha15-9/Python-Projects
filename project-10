import smtplib
import ssl
from email.message import EmailMessage
Email = "megha15092004@gmail.com"
app_password = "xbqiinqikxttgngv"
Reciever = "bhumisharma2786@gmail.com"
msg = EmailMessage()
msg["From"] = Email
msg["To"] = Reciever
msg["Subject"] = "hello"
msg.set_content("hi")
context = ssl.create_default_context()
with smtplib.SMTP_SSL("smtp.gmail.com" , 465 , context=context) as server:
    server.login(Email , app_password)
    server.send_message(msg)
